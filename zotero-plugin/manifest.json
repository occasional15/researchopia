{
  "manifest_version": 2,
  "name": "Academic Rating for Zotero",
  "version": "1.0.0",
  "description": "Integrates Academic Rating website functionality into Zotero Item Pane for research paper evaluation and rating.",
  "homepage_url": "https://www.researchopia.com",

  "applications": {
    "zotero": {
      "id": "academic-rating@researchopia.com",
      "update_url": "https://www.researchopia.com/zotero/updates.json",
      "strict_min_version": "7.0",
      "strict_max_version": "*"
    },
    "gecko": {
      "id": "academic-rating@researchopia.com",
      "strict_min_version": "78.0"
    }
  },

  "author": "Researchopia Team",

  "legacy": {
    "type": "bootstrap"
  },

  "icons": {
    "16": "icons/icon16.svg",
    "32": "icons/icon16.svg",
    "48": "icons/icon16.svg",
    "128": "icons/icon16.svg"
  }
}
